{
  "name": "sardorbek-furnitura",
  "version": "1.0.0",
  "description": "Sardorbek Furnitura - Kassa Tizimi",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "migrate": "cd server && node src/scripts/migrate-section-to-subcategory.js",
    "optimize": "cd server && node src/scripts/optimize-all-indexes.js"
  },
  "keywords": [
    "kassa",
    "pos",
    "inventory",
    "furnitura"
  ],
  "author": "Ozodbek",
  "license": "UNLICENSED",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
